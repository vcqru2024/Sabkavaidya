CREATE PROCEDURE {databaseOwner}[{objectQualifier}CP_Product_Tag_GetByTag]
	@Tag nvarchar(200),
    @TypeId int,
    @CultureCode nvarchar(20)
AS
Set rowcount 1

SELECT a.*,
       ''ProductCounts''=0
FROM {databaseOwner}{objectQualifier}CP_Product_Tag a
WHERE ([TypeId]=@TypeId)
and (Upper([Tag]) = Upper(@Tag))
and (@CultureCode='''' or [CultureCode]=@CultureCode)
	

' 
END
