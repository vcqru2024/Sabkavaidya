

/******   alter table CP_Product_Product ******/

alter table {databaseOwner}{objectQualifier}CP_Product_Product add ShippingProperty nvarchar(400) NULL

alter table {databaseOwner}{objectQualifier}CP_Product_Product add MinSellQuantity int NULL

go

/******   alter table CP_Product_Option ******/

alter table {databaseOwner}{objectQualifier}CP_Product_Option add StockQuantity int NULL
go

/******   alter table CP_Product_OrderDetail ******/

alter table {databaseOwner}{objectQualifier}CP_Product_OrderDetail add ShippingStatus int NULL

/******   alter table CP_Product_UDData ******/
alter table {databaseOwner}{objectQualifier}CP_Product_UDData add CultureCode nvarchar(30) NULL

/******   alter table CP_Product_Tag ******/
alter table {databaseOwner}{objectQualifier}CP_Product_Tag add CultureCode nvarchar(30) NULL

/******   alter table CP_Common_User ******/
alter table {databaseOwner}{objectQualifier}CP_Common_User add FavoriteShipMethod nvarchar(200) NULL

go

/******   set default values  ******/
Update {databaseOwner}{objectQualifier}CP_Product_Tag set CultureCode='' where CultureCode is null

go

update {databaseOwner}{objectQualifier}CP_Product_UDData set CultureCode='' where CultureCode is null

Go

