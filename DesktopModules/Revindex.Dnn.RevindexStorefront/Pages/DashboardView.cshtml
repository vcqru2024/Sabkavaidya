					<h3 class="panel-title card-title">@Html.LocalizeString("LowProductInventoryHeader")</h3>
				</div>
				<div class="panel-body card-body" style="min-height: 230px">
					<table class="table table-condensed table-sm">
						@foreach (var item in Model.LowProductInventoryReport)
						{
							<tr>
								<td><a href="@Global.RouteUrl(new NameValueCollection() { { Global.QueryStringKeys.PageTarget, "Catalog-ProductVariantControl" }, { Global.QueryStringKeys.ProductID, item.ProductID.ToString() }, { Global.QueryStringKeys.ProductVariantID, item.ProductVariantID.ToString() } })">@item.Product.Name</a></td>
								<td><strong>@item.Inventory</strong></td>
							</tr>
						}
					</table>
				</div>
			</div>
		</div>
	</div>
	<p class="text-muted mt-4">v@(Model.Version.ToString()) @Model.Edition</p>
	<input type="hidden" name="@Model.ViewStateName" value="@Model.ViewStateData" />
</div>