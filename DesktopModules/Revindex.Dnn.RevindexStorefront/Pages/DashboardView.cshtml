									value: @Model.SalesOrderStatusReport.TotalPaidSalesPayment,
									color: 'YellowGreen'
								},
								{
									category: '@HttpUtility.JavaScriptStringEncode(Html.LocalizeString("1", Global.FilePaths.LocalSalesPaymentStatusTypeResourceFile))',
									value: @Model.SalesOrderStatusReport.TotalPendingSalesPayment,
