	<div class="page-header pb-2 mt-4 mb-2 border-bottom">
		<h3>@Html.LocalizeString("SalesSnapshotHeader")</h3>
	</div>
	<div>
		<ul class="nav nav-pills" role="tablist">
			<li role="presentation" class="nav-item active"><a class="nav-link active" href="#rvdsfDailySalesSnapshotReport" role="tab" data-toggle="tab"><small>@Html.LocalizeString("TodayLabel")</small></a></li>
			<li role="presentation" class="nav-item"><a class="nav-link" href="#rvdsfWeeklySalesSnapshotReport" role="tab" data-toggle="tab"><small>@Html.LocalizeString("WeekLabel")</small></a></li>
			<li role="presentation" class="nav-item"><a class="nav-link" href="#rvdsfMonthlySalesSnapshotReport" role="tab" data-toggle="tab"><small>@Html.LocalizeString("MonthLabel")</small></a></li>
			<li role="presentation" class="nav-item"><a class="nav-link" href="#rvdsfQuarterlySalesSnapshotReport" role="tab" data-toggle="tab"><small>@Html.LocalizeString("QuarterLabel")</small></a></li>
			<li role="presentation" class="nav-item"><a class="nav-link" href="#rvdsfYearlySalesSnapshotReport" role="tab" data-toggle="tab"><small>@Html.LocalizeString("YearLabel")</small></a></li>
		</ul>
		<div class="tab-content">
			<div role="tabpanel" class="tab-pane active" id="rvdsfDailySalesSnapshotReport">
				<div class="row">
					<div class="col-sm-4">
						<div class="panel panel-default card" style="min-height: 160px">
							<div class="panel-heading card-header">
								<h3 class="panel-title card-title">@Html.LocalizeString("RevenueHeader")</h3>
							</div>
							<div class="panel-body card-body row">
								<div class="col-xs-8 col-8">
									<h4>@Model.DailySalesSnapshotReport.FormattedCurrentRevenue</h4>
									<label>@Html.LocalizeString("YesterdayLabel")</label>
									<p>@Model.DailySalesSnapshotReport.FormattedPreviousRevenue</p>
								</div>
								<div class="col-xs-4 col-4">
									@if (Model.DailySalesSnapshotReport.PercentChangeRevenue.HasValue)
									{
										<h4 style="color: @(Model.DailySalesSnapshotReport.PercentChangeRevenue < 0 ? "#FF5555" : "#00AA00"); word-break: break-all;">@Model.DailySalesSnapshotReport.PercentChangeRevenue.Value.ToString("+0.#;-0.#;0")%</h4>
									}
								</div>
							</div>
						</div>
					</div>
					<div class="col-sm-4">
						<div class="panel panel-default card" style="min-height: 160px">
							<div class="panel-heading card-header">
								<h3 class="panel-title card-title">@Html.LocalizeString("NumberOfOrdersHeader")</h3>
							</div>
							<div class="panel-body card-body row">
								<div class="col-xs-8 col-8">
									<h4>@Model.DailySalesSnapshotReport.CurrentNumberOfOrders</h4>
									<label>@Html.LocalizeString("YesterdayLabel")</label>
									<p>@Model.DailySalesSnapshotReport.PreviousNumberOfOrders</p>
								</div>
								<div class="col-xs-4 col-4">
									@if (Model.DailySalesSnapshotReport.PercentChangeNumberOfOrders.HasValue)
									{
										<h4 style="color: @(Model.DailySalesSnapshotReport.PercentChangeNumberOfOrders < 0 ? "#FF5555" : "#00AA00"); word-break: break-all;">@Model.DailySalesSnapshotReport.PercentChangeNumberOfOrders.Value.ToString("+0.#;-0.#;0")%</h4>
