@using System.Collections.Specialized;
@using DotNetNuke.Entities.Users;
@using Revindex.Business.Revindex.Revindex.Storefront;
@using Revindex.Data.Dnn;
@using Revindex.Dnn.RevindexStorefront.Pages;
@using Revindex.Web.Mvc.Dnn.Helpers;
@inherits Revindex.Web.Mvc.Dnn.RazorPages.DotNetNukeRazorPage<DashboardViewModel>

<div class="rvd-module-container rvdsf-dashboard-container">
	@if (!String.IsNullOrEmpty((string)Model.TempData.Peek(Global.TempDataKeys.Alert)) && !Model.IsRedirecting)
	{
		<div class="alert alert-@(Model.TempData[Global.TempDataKeys.Alert])">@Model.TempData[Global.TempDataKeys.Message]</div>
	}
	@if (Model.MissingModules.Count > 0)
	{
		<div class="alert alert-warning">@String.Format(Html.LocalizeString("ModuleMissingLabel"), @String.Join(", ", Model.MissingModules.ToArray())) <a href="@Global.RouteUrl(new NameValueCollection() { { Global.QueryStringKeys.PageTarget, "Configuration-InstallerControl" } })">@Html.LocalizeString("HelpMeLink")</a></div>
	}
	<h2>@Html.LocalizeString("DashboardHeader")</h2>
	<hr />
	<div class="row">
		<div class="col-sm-3">
			<div class="panel panel-default card">
				<div class="panel-heading card-header">
					<h3 class="panel-title card-title">@Html.LocalizeString("SalesOrderHealthHeader")</h3>
				</div>
				<div class="panel-body card-body">
					<div class="text-center" style="min-height: 50px">
						@{ var orderHealthy = true; }
						@if (Model.SalesOrderStatusReport.TotalPendingSalesOrder > 0)
						{
							orderHealthy = false;
							<a href="@Global.RouteUrl(new NameValueCollection() { { Global.QueryStringKeys.PageTarget, "Sales-SalesOrderControl" }, { Global.QueryStringKeys.SearchSalesOrderStatus, ((int)SalesOrderStatusType.Pending).ToString() }, { Global.QueryStringKeys.ReturnUrl, Request.RawUrl } })"><span class="label badge rvdsf-salesorder-status-1">@Model.SalesOrderStatusReport.TotalPendingSalesOrder @Html.LocalizeString("1", Global.FilePaths.LocalSalesOrderStatusTypeResourceFile)</span></a>
						}
						@if (Model.SalesOrderStatusReport.TotalQuotedSalesOrder > 0)
						{
							orderHealthy = false;
							<a href="@Global.RouteUrl(new NameValueCollection() { { Global.QueryStringKeys.PageTarget, "Sales-SalesOrderControl" }, { Global.QueryStringKeys.SearchSalesOrderStatus, ((int)SalesOrderStatusType.Quoted).ToString() }, { Global.QueryStringKeys.ReturnUrl, Request.RawUrl } })"><span class="label badge rvdsf-salesorder-status-9">@Model.SalesOrderStatusReport.TotalQuotedSalesOrder @Html.LocalizeString("9", Global.FilePaths.LocalSalesOrderStatusTypeResourceFile)</span></a>
						}
						@if (Model.SalesOrderStatusReport.TotalOrderedSalesOrder > 0)
						{
							orderHealthy = false;
							<a href="@Global.RouteUrl(new NameValueCollection() { { Global.QueryStringKeys.PageTarget, "Sales-SalesOrderControl" }, { Global.QueryStringKeys.SearchSalesOrderStatus, ((int)SalesOrderStatusType.Ordered).ToString() }, { Global.QueryStringKeys.ReturnUrl, Request.RawUrl } })"><span class="label badge rvdsf-salesorder-status-2">@Model.SalesOrderStatusReport.TotalOrderedSalesOrder @Html.LocalizeString("2", Global.FilePaths.LocalSalesOrderStatusTypeResourceFile)</span></a>
						}
						@if (Model.SalesOrderStatusReport.TotalProcessingSalesOrder > 0)
						{
							orderHealthy = false;
							<a href="@Global.RouteUrl(new NameValueCollection() { { Global.QueryStringKeys.PageTarget, "Sales-SalesOrderControl" }, { Global.QueryStringKeys.SearchSalesOrderStatus, ((int)SalesOrderStatusType.Processing).ToString() }, { Global.QueryStringKeys.ReturnUrl, Request.RawUrl } })"><span class="label badge rvdsf-salesorder-status-3">@Model.SalesOrderStatusReport.TotalProcessingSalesOrder @Html.LocalizeString("3", Global.FilePaths.LocalSalesOrderStatusTypeResourceFile)</span></a>
						}
						@if (orderHealthy)
						{
							<span>@Html.LocalizeString("GoodLabel")</span>
						}
					</div>
					<div id="rvdsfSalesOrderStatusPanel"></div>
					<script>
						$(document).ready(function () {
							var data = [
								{
									category: '@HttpUtility.JavaScriptStringEncode(Html.LocalizeString("5", Global.FilePaths.LocalSalesOrderStatusTypeResourceFile))',
									value: @Model.SalesOrderStatusReport.TotalCancelledSalesOrder,
									color: 'Khaki'
								},
								{
									category: '@HttpUtility.JavaScriptStringEncode(Html.LocalizeString("4", Global.FilePaths.LocalSalesOrderStatusTypeResourceFile))',
									value: @Model.SalesOrderStatusReport.TotalCompletedSalesOrder,
									color: 'YellowGreen'
								},
								{
									category: '@HttpUtility.JavaScriptStringEncode(Html.LocalizeString("6", Global.FilePaths.LocalSalesOrderStatusTypeResourceFile))',
									value: @Model.SalesOrderStatusReport.TotalDeclinedSalesOrder,
									color: 'Red'
								},
								{
									category: '@HttpUtility.JavaScriptStringEncode(Html.LocalizeString("2", Global.FilePaths.LocalSalesOrderStatusTypeResourceFile))',
									value: @Model.SalesOrderStatusReport.TotalOrderedSalesOrder,
									color: 'Gold'
								},
								{
									category: '@HttpUtility.JavaScriptStringEncode(Html.LocalizeString("1", Global.FilePaths.LocalSalesOrderStatusTypeResourceFile))',
									value: @Model.SalesOrderStatusReport.TotalPendingSalesOrder,
									color: 'HotPink'
								},
								{
									category: '@HttpUtility.JavaScriptStringEncode(Html.LocalizeString("3", Global.FilePaths.LocalSalesOrderStatusTypeResourceFile))',
									value: @Model.SalesOrderStatusReport.TotalProcessingSalesOrder,
									color: 'SkyBlue'
								},
								{
									category: '@HttpUtility.JavaScriptStringEncode(Html.LocalizeString("9", Global.FilePaths.LocalSalesOrderStatusTypeResourceFile))',
									value: @Model.SalesOrderStatusReport.TotalQuotedSalesOrder,
									color: 'DeepPink'
								},
							]
							

							if (Math.max.apply(Math, data.map(function(o) { return o.value; })) == 0) {
								data = [{
									category: 'No data',
									value: 1,
									color: 'Silver'
								}]
							}

							$("#rvdsfSalesOrderStatusPanel").kendoChart({
								chartArea: {
									height: 190,
									margin: 0
								},
								legend: {
									visible: false
								},
								seriesDefaults: {
									type: "donut"
								},
								series: [{ name: "", data: data }],
								tooltip: {
									visible: true,
									template: "#= category #: #= value #"
								}
							});
						})
					</script>
				</div>
			</div>
		</div>
