/** Drop  Function **/

/****** Object:  UserDefinedFunction {databaseOwner}[{objectQualifier}CP_CsvToInt]    ScriptDate:08/15/2010 23:05:44 ******/
IF  EXISTS (SELECT * FROM sys.objects WHERE object_id = OBJECT_ID(N'{databaseOwner}[CP_CsvToInt]') AND type in (N'FN', N'IF', N'TF', N'FS', N'FT'))
DROP FUNCTION {databaseOwner}[CP_CsvToInt]
GO

